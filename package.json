{
  "name": "@tadashi/svelte-splash-lgpd",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lagden/svelte-splash-lgpd.git"
  },
  "description": "Svelte components",
  "keywords": [
    "svelte",
    "elements",
    "lgpd",
    "splash",
    "component"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "homepage": "https://github.com/lagden/svelte-splash-lgpd#readme",
  "bugs": {
    "url": "https://github.com/lagden/svelte-splash-lgpd/issues"
  },
  "engines": {
    "node": ">=14.16"
  },
  "type": "module",
  "svelte": "src/Splash.svelte",
  "main": "dist/splash-lgpd.js",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "eslint": "eslint --config .eslintrc.cjs --ext .js --ignore-path .gitignore .",
    "rm": "rm -rf dist",
    "rollup": "rollup -c",
    "rollupw": "npm run rollup -- -w",
    "build": "run-s test rm rollup",
    "prepublishOnly": "npm run build",
    "pretest": "npm run eslint",
    "test": "jest test --coverage --coverageReporters=lcov --coverageReporters=text",
    "test:watch": "npm test -- --watch",
    "server": "sirv public --single --host 0.0.0.0 --dev",
    "start": "run-p server rollupw"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/eslint-parser": "7.14.7",
    "@babel/plugin-syntax-top-level-await": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@rollup/plugin-commonjs": "19.0.1",
    "@rollup/plugin-node-resolve": "13.0.2",
    "@tadashi/flatten-object": "2.0.0",
    "@tadashi/unflatten-object": "1.0.2",
    "@testing-library/svelte": "3.0.3",
    "babel-jest": "27.0.6",
    "eslint": "7.31.0",
    "eslint-config-xo": "0.37.0",
    "eslint-plugin-html": "6.1.2",
    "eslint-plugin-svelte3": "3.2.0",
    "eslint-plugin-unicorn": "34.0.1",
    "jest": "27.0.6",
    "node-fetch": "2.6.1",
    "npm-run-all": "4.1.5",
    "rollup": "2.53.2",
    "rollup-plugin-svelte": "7.1.0",
    "sirv-cli": "1.0.12",
    "svelte": "3.38.3",
    "svelte-jester": "1.7.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-top-level-await"
    ]
  }
}
